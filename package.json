    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "next": "~15.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0",
    "redis": "^5.5.6",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.8.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/devkit": "21.1.2",
    "@nx/esbuild": "21.1.3",
    "@nx/eslint": "21.1.2",
    "@nx/eslint-plugin": "21.1.3",
    "@nx/jest": "21.1.2",
    "@nx/js": "21.1.3",
    "@nx/next": "^21.1.3",
