{
  "name": "@salambot-atlas/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:libs": "tsc --noEmit --project tsconfig.lib.json",
    "build:types": "tsc --project tsconfig.lib.json",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --coverage",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.config.ts --testNamePattern='integration'",
    "test:e2e": "nx run-many --target=e2e --all",
    "code-quality": "pnpm run lint && pnpm run format:check && pnpm run type-check",
    "code-quality:fix": "pnpm run lint:fix && pnpm run format && pnpm run type-check",
    "pre-commit": "pnpm run code-quality:fix"
  },
  "private": true,
  "dependencies": {
    "@google-cloud/firestore": "^7.11.1",
    "@google-cloud/storage": "^7.16.0",
    "axios": "^1.6.0",
